{
 "coc.preferences.timeout": 3000,


  "explorer.width": 30,
  "explorer.icon.enableNerdfont": false,
  "explorer.previewAction.onHover": false,
  "explorer.icon.enableVimDevicons": false,
  "explorer.file.showHiddenFiles": false,
  
  "explorer.icon.expanded": "-",
  "explorer.icon.collapsed": "+",

  "explorer.file.column.indent.indentLine": true,
  "explorer.keyMappings": {
   "<cr>": ["expandable?", "expand", "open"],
    "v": "open:vsplit"
  },

  "flutter.provider.hot-reload": false,
}
